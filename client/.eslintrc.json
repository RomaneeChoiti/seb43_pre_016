{
  "env": {
    "browser": true
  },
    "extends" : [        
        "plugin:react/recommended",
        "plugin:import/recommended",
        "plugin:prettier/recommended",
        "plugin:react/jsx-runtime"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
     "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
  "react/prop-types": "off"
    }
    
}

